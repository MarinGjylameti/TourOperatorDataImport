{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "TourOperatorDataImport.Application": "Information"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=127.0.0.1,1433;Database=TourOperator;User Id=sa;Password=YourPassword123!;TrustServerCertificate=true",
    "Redis": "localhost:6379"
  },
  "ApiDocumentationOptions": {
    "ApiName": "Tour Operator Data Import API"
  },
  "Jwt": {
    "Key": "YourSuperSecretKeyThatIsAtLeast32CharactersLong!",
    "Issuer": "TourOperatorAPI",
    "Audience": "TourOperatorClient"
  },
  "AllowedHosts": "*"
}
